<UserControl x:Class="RealEstateDatabaseApp.SerwisanciForm"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:RealEstateDatabaseApp"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <StackPanel>
            <TextBox x:Name="TxtHandymanType" local:TextBoxPlaceholder.PlaceholderText="Typ serwisanta" Margin="5" />
            <TextBox x:Name="TxtName" local:TextBoxPlaceholder.PlaceholderText="Imie" Margin="5" />
            <TextBox x:Name="TxtSurname" local:TextBoxPlaceholder.PlaceholderText="Nazwisko" Margin="5" />
            <TextBox x:Name="TxtContact" local:TextBoxPlaceholder.PlaceholderText="Kontakt" Margin="5" />
            <Button Content="Dodaj Serwisanta" Click="AddSerwisant" Margin="5" />
            <Button Content="Usuń Serwisanta" Click="DeleteSerwisant" Margin="5" />
        </StackPanel>
        
    </Grid>
</UserControl>